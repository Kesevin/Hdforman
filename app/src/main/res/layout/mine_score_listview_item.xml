<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ratingbar="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/colorBackground">
    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="180px"
        android:orientation="vertical"
        android:paddingLeft="24px"
        android:gravity="center_vertical"
        android:layout_marginTop="20px"
        android:background="@android:color/white">
        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/score_item_project_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="中德英伦联邦"
                android:textColor="@color/textDarkColor"/>
            <TextView
                android:id="@+id/score_item_project_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="12栋1单元1004号"
                android:textColor="@color/textLightDarkColor"
                android:textSize="@dimen/def_text_size_px"
                android:layout_marginLeft="22px"/>
        </com.zhy.autolayout.AutoLinearLayout>
        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="26px">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="完工时间："
                android:textColor="@color/textLightDarkColor"
                android:textSize="@dimen/def_text_size_px"/>
            <TextView
                android:id="@+id/score_item_project_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2016-08-12"
                android:textColor="@color/textDarkColor"
                android:textSize="@dimen/def_text_size_px"/>
        </com.zhy.autolayout.AutoLinearLayout>
    </com.zhy.autolayout.AutoLinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="2px"
        android:background="@color/colorDivide"/>
    <com.zhy.autolayout.AutoLinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:paddingLeft="24px"
        android:paddingBottom="20px">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/score_item_goal"
            android:textColor="@color/textDarkColor"
            android:layout_marginTop="40px"
            android:layout_marginBottom="40px"
            android:textSize="@dimen/def_text_size_sp"/>
        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="542px"
                android:layout_height="326px"
                android:orientation="vertical">
                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="38px"
                        android:layout_height="wrap_content"
                        android:src="@mipmap/score_list_coordinate_normal"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/score_item_list1"
                        android:layout_marginLeft="16px"/>
                    <!--<RatingBar
                        android:id="@+id/score_item_ratingBar_harmony"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:numStars="5"
                        android:rating="4.7"
                        style="@style/scoreRatingBar"
                        android:layout_marginLeft="30px"
                        android:isIndicator="true"
                        android:stepSize="0.1"/>-->
                    <com.dgg.hdforeman.app.view.StarBar
                        android:id="@+id/score_item_ratingBar_harmony"
                        android:layout_below="@+id/display"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        ratingbar:starEmpty="@mipmap/list_star_normal"
                        ratingbar:starFill="@mipmap/list_star_pressed"
                        ratingbar:starDistance="5dp"
                        ratingbar:starCount="5"
                        ratingbar:starSize="48px"
                        android:layout_marginLeft="30px"
                        ratingbar:clickEnable="false"/>
                </com.zhy.autolayout.AutoLinearLayout>
                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginTop="36px"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:layout_weight="1">

                    <ImageView
                        android:layout_width="38px"
                        android:layout_height="36px"
                        android:src="@mipmap/score_list_service_normal"
                        android:id="@+id/imageView" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/score_item_list2"
                        android:layout_marginLeft="16px"/>
                    <!--<RatingBar
                        android:id="@+id/score_item_ratingBar_service"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:numStars="5"
                        android:rating="3"
                        style="@style/scoreRatingBar"
                        android:layout_marginLeft="30px"
                        android:isIndicator="true"
                        android:stepSize="0.1"/>-->
                    <com.dgg.hdforeman.app.view.StarBar
                        android:id="@+id/score_item_ratingBar_service"
                        android:layout_below="@+id/display"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        ratingbar:starEmpty="@mipmap/list_star_normal"
                        ratingbar:starFill="@mipmap/list_star_pressed"
                        ratingbar:starDistance="5dp"
                        ratingbar:starCount="5"
                        ratingbar:starSize="48px"
                        android:layout_marginLeft="30px"
                        ratingbar:clickEnable="false"/>

                </com.zhy.autolayout.AutoLinearLayout>
                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginTop="36px"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="38px"
                        android:layout_height="36px"
                        android:src="@mipmap/score_list_project_controls_normal"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/score_item_list3"
                        android:layout_marginLeft="16px"/>
                    <!--<RatingBar
                        android:id="@+id/score_item_ratingBar_date"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:numStars="5"
                        android:rating="2"
                        style="@style/scoreRatingBar"
                        android:layout_marginLeft="30px"
                        android:isIndicator="true"
                        android:stepSize="0.1"/>-->
                    <com.dgg.hdforeman.app.view.StarBar
                        android:id="@+id/score_item_ratingBar_date"
                        android:layout_below="@+id/display"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        ratingbar:starEmpty="@mipmap/list_star_normal"
                        ratingbar:starFill="@mipmap/list_star_pressed"
                        ratingbar:starDistance="5dp"
                        ratingbar:starCount="5"
                        ratingbar:starSize="46px"
                        android:layout_marginLeft="30px"
                        ratingbar:clickEnable="false"/>
                </com.zhy.autolayout.AutoLinearLayout>
                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginTop="36px"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="38px"
                        android:layout_height="36px"
                        android:src="@mipmap/score_list_skills_normal"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/score_item_list4"
                        android:layout_marginLeft="16px"/>
                    <!--<RatingBar
                        android:id="@+id/score_item_ratingBar_skill"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:numStars="5"
                        android:rating="1"
                        style="@style/scoreRatingBar"
                        android:layout_marginLeft="30px"
                        app:layout_auto_basewidth="height"
                        android:isIndicator="true"
                        android:stepSize="0.1"/>-->
                    <com.dgg.hdforeman.app.view.StarBar
                        android:id="@+id/score_item_ratingBar_skill"
                        android:layout_below="@+id/display"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        ratingbar:starEmpty="@mipmap/list_star_normal"
                        ratingbar:starFill="@mipmap/list_star_pressed"
                        ratingbar:starDistance="5dp"
                        ratingbar:starCount="5"
                        ratingbar:starSize="46px"
                        android:layout_marginLeft="30px"
                        ratingbar:clickEnable="false"/>

                </com.zhy.autolayout.AutoLinearLayout>
            </com.zhy.autolayout.AutoLinearLayout>
            <TextView
                android:id="@+id/score_item_score"
                android:layout_width="match_parent"
                android:layout_height="326px"
                android:gravity="center"
                android:text="4.7"
                android:textSize="40sp"
                android:textColor="@color/colorOrange"
                android:textStyle="italic"/>
        </com.zhy.autolayout.AutoLinearLayout>
    </com.zhy.autolayout.AutoLinearLayout>
</com.zhy.autolayout.AutoLinearLayout>