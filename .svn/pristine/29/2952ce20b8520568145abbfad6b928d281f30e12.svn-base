<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBackground"
    android:orientation="vertical">

    <com.dgg.hdforeman.mvp.ui.widget.autolayout.AutoToolbar
        android:id="@+id/toolbar"
        style="@style/toolbar_style"
        app:contentInsetStart="0dp">

        <include layout="@layout/title_bar_layout" />
    </com.dgg.hdforeman.mvp.ui.widget.autolayout.AutoToolbar>

    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/announcement_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_height"
        android:layout_marginTop="@dimen/top_spacing"
        android:background="@drawable/common_layout_press_selector"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="@dimen/left_right_spacing">

        <TextView
            style="@style/tv_dark_style"
            android:drawableLeft="@mipmap/announcement"
            android:drawablePadding="12px"
            android:lines="1"
            android:text="公告" />
        <TextView
            android:id="@+id/right_arrow"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableRight="@mipmap/jump"
            />

    </com.zhy.autolayout.AutoLinearLayout>

    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/biaozhun_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_height"
        android:layout_marginTop="20px"
        android:background="@drawable/common_layout_press_selector"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="@dimen/left_right_spacing">

        <TextView
            style="@style/tv_dark_style"
            android:drawableLeft="@mipmap/biaozhun"
            android:drawablePadding="12px"
            android:lines="1"
            android:text="基础装修施工工艺及验收标准" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableRight="@mipmap/jump"
            />
    </com.zhy.autolayout.AutoLinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/divide_line_height"
        android:background="@color/colorDivide" />

    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/zhucai_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_height"
        android:background="@drawable/common_layout_press_selector"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="@dimen/left_right_spacing">

        <TextView
            style="@style/tv_dark_style"
            android:drawableLeft="@mipmap/zhucai"
            android:drawablePadding="12px"
            android:lines="1"
            android:text="主材安装施工工艺及验收标准" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableRight="@mipmap/jump"
            />

    </com.zhy.autolayout.AutoLinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/divide_line_height"
        android:background="@color/colorDivide" />

    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/jichubao_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_height"
        android:background="@drawable/common_layout_press_selector"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="@dimen/left_right_spacing">

        <TextView
            style="@style/tv_dark_style"
            android:drawableLeft="@mipmap/jichubao"
            android:drawablePadding="12px"
            android:lines="1"
            android:text="标准基础包" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableRight="@mipmap/jump"
            />
    </com.zhy.autolayout.AutoLinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/divide_line_height"
        android:background="@color/colorDivide" />

    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/mingxibiao_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_height"
        android:background="@drawable/common_layout_press_selector"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="@dimen/left_right_spacing">

        <TextView
            style="@style/tv_dark_style"
            android:drawableLeft="@mipmap/mingxibiao"
            android:drawablePadding="12px"
            android:lines="1"
            android:text="个性项报价明细表" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableRight="@mipmap/jump"
            />
    </com.zhy.autolayout.AutoLinearLayout>
</com.zhy.autolayout.AutoLinearLayout>