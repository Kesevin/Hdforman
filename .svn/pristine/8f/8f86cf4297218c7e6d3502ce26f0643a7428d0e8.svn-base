<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBackground">
    <include
        layout="@layout/title_bar_layout"/>
    
    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="428px"
        android:orientation="vertical"
        android:layout_marginTop="20px"
        android:background="@android:color/white">
        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:orientation="vertical"
            android:paddingLeft="24px"
            android:paddingRight="24px">
            <!--<com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:visibility="gone">
                <TextView
                    android:layout_width="112px"
                    android:layout_height="wrap_content"
                    android:text="@string/data_workType"
                    android:textColor="@color/textLightDarkColor"
                    android:textSize="28px"/>
                <TextView
                    android:id="@+id/data_workType"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="水电工"
                    android:layout_marginLeft="90px"
                    android:textSize="@dimen/def_text_size_px"
                    android:textColor="@color/textDarkColor"
                    android:gravity="center_vertical"/>
            </com.zhy.autolayout.AutoLinearLayout>-->
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="112px"
                    android:layout_height="wrap_content"
                    android:text="@string/data_fitment"
                    android:textColor="@color/textLightDarkColor"
                    android:textSize="28px"/>
                <TextView
                    android:id="@+id/data_fitment"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="15年"
                    android:layout_marginLeft="90px"
                    android:textSize="@dimen/def_text_size_px"
                    android:textColor="@color/textDarkColor"
                    android:gravity="center_vertical"/>
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="112px"
                    android:layout_height="wrap_content"
                    android:text="@string/data_address"
                    android:textColor="@color/textLightDarkColor"
                    android:textSize="28px"/>
                <TextView
                    android:id="@+id/data_address"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="奥克斯广场"
                    android:layout_marginLeft="90px"
                    android:textSize="@dimen/def_text_size_px"
                    android:textColor="@color/textDarkColor"
                    android:gravity="center_vertical"/>
            </com.zhy.autolayout.AutoLinearLayout>
        </com.zhy.autolayout.AutoLinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:background="@color/colorDivide"/>
        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="3"
            android:orientation="vertical"
            android:paddingLeft="24px"
            android:paddingRight="24px">
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="112px"
                    android:layout_height="wrap_content"
                    android:text="@string/data_cardNumber"
                    android:textColor="@color/textLightDarkColor"
                    android:textSize="28px"/>
                <TextView
                    android:id="@+id/data_cardNumber"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="513624******1547896"
                    android:layout_marginLeft="90px"
                    android:textSize="@dimen/def_text_size_px"
                    android:textColor="@color/textDarkColor"
                    android:gravity="center_vertical"/>
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="112px"
                    android:layout_height="wrap_content"
                    android:text="@string/data_phoneNumber"
                    android:textColor="@color/textLightDarkColor"
                    android:textSize="28px"/>
                <TextView
                    android:id="@+id/data_phoneNumber"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="136****5656"
                    android:layout_marginLeft="90px"
                    android:textSize="@dimen/def_text_size_px"
                    android:textColor="@color/textDarkColor"
                    android:gravity="center_vertical"/>
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="112px"
                    android:layout_height="wrap_content"
                    android:text="@string/data_bankNumber"
                    android:textColor="@color/textLightDarkColor"
                    android:textSize="28px"/>
                <TextView
                    android:id="@+id/data_bankNumber"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="564836 1235*****5625"
                    android:layout_marginLeft="90px"
                    android:textSize="@dimen/def_text_size_px"
                    android:textColor="@color/textDarkColor"
                    android:gravity="center_vertical"/>
            </com.zhy.autolayout.AutoLinearLayout>
        </com.zhy.autolayout.AutoLinearLayout>
    </com.zhy.autolayout.AutoLinearLayout>
</com.zhy.autolayout.AutoLinearLayout>