<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_received_order"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.zhy.autolayout.AutoRelativeLayout
        android:id="@+id/rl_project_info"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20px"
        android:background="@color/textWhiteColor">

        <TextView
            android:id="@+id/tv_project_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="26px"
            android:layout_marginTop="32px"
            android:text="项目信息"
            android:textColor="@color/textLightDarkColor"
            android:textSize="@dimen/text_size_17" />

        <TextView
            android:id="@+id/tv_owner_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_project_info"

            android:layout_marginLeft="26px"
            android:layout_marginTop="36px"
            android:text="李先生 /122 m²"
            android:textColor="@color/textDarkColor"
            android:textSize="@dimen/text_size_19" />

        <TextView
            android:id="@+id/tv_project_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_owner_name"
            android:layout_marginBottom="38px"
            android:layout_marginLeft="26px"
            android:layout_marginTop="36px"
            android:text="中德英伦城邦 12栋1单元1004号"
            android:textColor="@color/textLightDarkColor"
            android:textSize="@dimen/text_size_19" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="24px"
            android:background="@android:color/transparent"
            android:src="@mipmap/list_jump" />
    </com.zhy.autolayout.AutoRelativeLayout>

    <com.zhy.autolayout.AutoRelativeLayout
        android:id="@+id/rl_site_survey"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/toolbar"
        android:layout_marginTop="20px"
        android:background="@color/textWhiteColor">

        <TextView
            android:id="@+id/tv_site_survey"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="26px"
            android:layout_marginTop="32px"
            android:text="工地测量"
            android:textColor="@color/textLightDarkColor"
            android:textSize="@dimen/text_size_17" />

        <TextView
            android:id="@+id/tv_total"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_site_survey"
            android:layout_marginBottom="40px"
            android:layout_marginLeft="26px"
            android:layout_marginTop="36px"
            android:text="合计 : 待测量"
            android:textColor="@color/textDarkColor"
            android:textSize="@dimen/text_size_19" />


        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="24px"
            android:background="@android:color/transparent"
            android:src="@mipmap/list_jump" />
    </com.zhy.autolayout.AutoRelativeLayout>

    <com.zhy.autolayout.AutoRelativeLayout
        android:id="@+id/rl_project_quotation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20px"
        android:background="@color/textWhiteColor">

        <TextView
            android:id="@+id/tv_project_quotation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="26px"
            android:layout_marginTop="32px"
            android:text="项目报价"
            android:textColor="@color/textLightDarkColor"
            android:textSize="@dimen/text_size_17" />

        <TextView
            android:id="@+id/tv_quotation_total"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_project_quotation"
            android:layout_marginBottom="40px"
            android:layout_marginLeft="26px"
            android:layout_marginTop="36px"
            android:text="合计 : 待测量"
            android:textColor="@color/textDarkColor"
            android:textSize="@dimen/text_size_19" />


        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="24px"
            android:background="@android:color/transparent"
            android:src="@mipmap/list_jump" />
    </com.zhy.autolayout.AutoRelativeLayout>
</com.zhy.autolayout.AutoLinearLayout>